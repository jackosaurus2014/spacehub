{
  "name": "spacehub",
  "version": "0.8.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma db push || echo '⚠️  WARNING: prisma db push failed — schema may be out of sync' && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "postinstall": "prisma generate",
    "generate-icons": "tsx scripts/generate-icons.ts",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "0.72.1",
    "@auth/prisma-adapter": "1.4.0",
    "@capacitor/app": "^6.0.3",
    "@capacitor/core": "^6.2.1",
    "@capacitor/haptics": "^6.0.3",
    "@capacitor/ios": "^6.2.1",
    "@capacitor/keyboard": "^6.0.4",
    "@capacitor/local-notifications": "^6.1.3",
    "@capacitor/push-notifications": "^6.0.5",
    "@capacitor/share": "^6.0.4",
    "@capacitor/splash-screen": "^6.0.4",
    "@capacitor/status-bar": "^6.0.3",
    "@capawesome/capacitor-badge": "^6.0.0",
    "@prisma/client": "5.8.0",
    "@react-three/drei": "9.122.0",
    "@react-three/fiber": "8.18.0",
    "bcryptjs": "2.4.3",
    "capacitor-native-biometric": "^4.2.2",
    "framer-motion": "^12.33.0",
    "next": "^14.2.35",
    "next-auth": "4.24.5",
    "node-cron": "^4.2.1",
    "nodemailer": "7.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "resend": "6.9.1",
    "rss-parser": "3.13.0",
    "sanitize-html": "2.17.0",
    "stripe": "^17.7.0",
    "three": "0.182.0",
    "yahoo-finance2": "3.13.0",
    "zod": "4.3.6"
  },
  "overrides": {
    "next-auth": {
      "nodemailer": "$nodemailer"
    },
    "@auth/core": {
      "nodemailer": "$nodemailer"
    }
  },
  "devDependencies": {
    "@capacitor/cli": "^6.2.1",
    "@jest/globals": "29.7.0",
    "@next/bundle-analyzer": "^16.1.6",
