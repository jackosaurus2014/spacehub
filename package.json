    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/sanitize-html": "2.16.0",
    "@types/three": "0.182.0",
    "autoprefixer": "10.4.17",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "postcss": "8.4.33",
    "prisma": "5.8.0",
    "sharp": "0.33.5",
    "tailwindcss": "3.4.1",
    "ts-jest": "29.2.0",
    "ts-node": "10.9.2",
    "tsx": "4.21.0",
    "typescript": "5.3.3"
  }
